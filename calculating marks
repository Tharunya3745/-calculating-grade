#include<stdio.h>
int main()
{
    int i;
    float mark, sum=0, avg;
    printf("Enter Marks obtained in 5 Subjects\n: ");
    for(i=0; i<5; i++)
    {
        scanf("%f", &mark);
        sum = sum+mark;
    }
    avg = sum/5;
    printf("\nGrade = ");
    if(avg>=91 && avg<=100)
        printf("A");
   
    else if(avg>=80 && avg<81)
        printf("B");
    else if(avg>=70 && avg<71)
        printf("C");
    else if(avg>=60 && avg<61)
        printf("D");
    else if(avg>=50&& avg<51)
        printf("E");
    
    else
        printf("Invalid!");
   
    return 0;
}




